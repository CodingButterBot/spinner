{
  "name": "spin-pick",
  "description": "SpinPick is the ultimate randomizer built for modern raffle companies and creative giveaways. Whether you prefer the charm of a spinning wheel, the thrill of a slot machine, or the suspense of shuffling name cards, SpinPick lets you customize your draw with professional polish and visual flair.",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --config nodemon.chrome.json",
    "dev:log": "cat .vite.log",
    "dev:stop": "pkill -f vite",
    "build": "vite build --config vite.config.chrome.ts",
    "build:prod": "vite build --config vite.config.prod.ts --mode production --force",
    "package": "pnpm run build:prod && cd dist_chrome && zip -r ../spinpick-extension.zip .",
    "clean": "rimraf dist dist_chrome dist_firefox",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "node scripts/run-tests.js",
    "test:components": "node scripts/run-component-tests.js"
  },
  "dependencies": {
    "@directus/sdk": "^19.1.0",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tailwindcss/vite": "^4.1.5",
    "@types/papaparse": "^5.3.16",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.508.0",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.3",
    "react-icons": "^5.5.0",
    "react-reel": "^1.2.3",
    "react-router-dom": "^7.6.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.22",
